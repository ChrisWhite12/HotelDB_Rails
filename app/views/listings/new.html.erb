<div class="page_sec">
    <%= form_with(model: @listing, local: true) do |form| %>
        <div>
            <%= form.label :name %>
            <%= form.text_field :name %>
        </div>
        <div>
            <%= form.label :location %>
            <%= form.text_field :location %>
        </div>
        <div>
            <%= form.label :picture, value="Image" %>
            <%= form.file_field :picture %>
        </div>
        <%= form.submit %>
    <% end %>
</div>